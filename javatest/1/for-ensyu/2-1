public class Main {
    public static void main(String[] args) {
        calc(10);
        calc(100);
    }
    public static void calc(int number) {
        int sum = 0;
        for(int i = 0; i <= number; i++) {
            sum += i;
        }
        System.out.println("1から" + number + "の和は" + sum);
        
    }
}